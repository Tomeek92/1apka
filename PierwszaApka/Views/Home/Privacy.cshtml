@model List<PoCoTaApka>




@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>



@{
    var dzisiejszaData = DateTime.Now.DayOfWeek;

    <p>@dzisiejszaData</p>

    if (dzisiejszaData == DayOfWeek.Saturday|| dzisiejszaData == DayOfWeek.Sunday)
    {
        <p>It's a weekend ;)</p>
    }
    else
    {
        <p>Go Work!</p>
    }

    


}
@foreach(var person in Model)
{
    <p>@person.JakasZmienna
    @person.JakasZmiennaString</p>
}

    
